<!-- 
	common overrides for all geonetwork files   
-->
<overrides>
	<textFile name=".*/WEB-INF/server.prop">
		<update linePattern="buildProfile=geonetwork.build.profile">buildProfile=prod</update>
		<update linePattern="subVersion=geonetwork.subversion">subVersion=SNAPSHOT</update>
		<update linePattern="version=geonetwork.version">version=2.10.3-SNAPSHOT</update>
		<update linePattern="release=geonetwork.version">release=2.10.3-SNAPSHOT</update>
		<update linePattern="javaVersion=java.version">javaVersion=2.10.3-SNAPSHOT</update>
		<update linePattern="OS=os">OS=Linux 3.16.0-4-amd64 amd64</update>
	</textFile>
</overrides>